onEvent("startOverButton", "click", function(event) {
  console.log("You are now starting over.");
  setScreen("screen1");
  playSound("sound://category_achievements/vibrant_game_postive_achievement_3.mp3", false);
});
onEvent("playAgainButton", "click", function(event) {
  console.log("You are now playing again.");
  setScreen("screen1");
  playSound("sound://category_achievements/vibrant_game_postive_achievement_3.mp3", false);
});
onEvent("appleButton", "click", function(event) {
  console.log("Correct!");
  setScreen("screen2");
  playSound("sound://category_achievements/lighthearted_bonus_objective_5.mp3", false);
});
